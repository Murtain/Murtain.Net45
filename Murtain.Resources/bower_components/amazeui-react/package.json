  "author": "AllMobilize, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amazeui/amazeui-react/issues"
  },
  "homepage": "https://github.com/amazeui/amazeui-react",
  "dependencies": {
