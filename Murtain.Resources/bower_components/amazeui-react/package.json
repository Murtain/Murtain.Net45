{
  "name": "amazeui-react",
  "title": "Amaze UI React",
  "version": "1.2.2",
  "description": "Amaze UI components build with React.",
  "main": "lib/AMUIReact.js",
  "scripts": {
    "test": "jest",
    "start": "gulp",
