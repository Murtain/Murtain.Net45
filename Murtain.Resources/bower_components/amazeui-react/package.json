    "examples": "node ./examples/server.js",
    "changelog": "github-changes -o amazeui -r amazeui-react -a"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/amazeui/amazeui-react"
  },
  "keywords": [
    "react",
    "react-component",
    "amazeui",
    "react.js",
    "amazeui",
    "amazeui-react",
    "react-amazeui",
    "web-components"
