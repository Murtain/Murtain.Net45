    "brfs": "^1.4.3",
    "browser-sync": "^2.14.0",
    "codemirror": "^5.17.0",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "del": "^2.2.1",
    "eslint": "^3.2.2",
    "eslint-plugin-react": "^6.0.0",
    "file-loader": "^0.9.0",
